		"@typescript-eslint/parser": "^5.42.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.26.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.8.4"
	},
	"extensionDependencies": [
		"sumneko.lua"
